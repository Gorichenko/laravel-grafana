{
    "name": "nix-enterprise/laravel-grafana-datasource",
    "version": "0.0.1",
    "description": "Grafana datasource",
    "type": "library",
    "authors": [
        {
            "name": "Evgeniy Gorichenko",
            "email": "evgeniy.gorichenko@nixsolutions.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "laravel/framework": ">=5.8"
    },
    "autoload": {
        "psr-4": {
            "NixEnterprise\\GrafanaDatasource\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NixEnterprise\\GrafanaDatasource\\GrafanaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
